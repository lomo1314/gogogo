}

.tc{
  text-align: center;
}

.page input{
  padding: 20rpx 30rpx;
  background-color: #fff;
}
checkbox, radio{
  margin-right: 10rpx;
}

.btn-area{
  padding: 0 30px;
}
.btn-area button{
  margin-top: 20rpx;
  margin-bottom: 20rpx;
}

.page {
	min-height: 100%;
	flex: 1;
	background-color: #FBF9FE;
	font-size: 32rpx;
	font-family: -apple-system-font,Helvetica Neue,Helvetica,sans-serif;
	overflow: hidden;
}
.page__hd{
	padding: 50rpx 50rpx 100rpx 50rpx;
	text-align: center;
}
.page__title{
	display: inline-block;
	padding: 20rpx 40rpx;
	font-size: 32rpx;
	color: #AAAAAA;
	border-bottom: 1px solid #CCCCCC;
}

.list{
	background-color:#FFFFFF;
  border-bottom:1px solid #eeeeee;
}

/*加载更多*/
.loading-list{
	height: 40px;
	/* display: flex; */
	justify-content: center;
  align-items: center;
  text-align: center;
}
.loading-gif{
	height: 16px;
	width: 16px;	
	margin-right:10px;
}
/*加载失败*/
.wifihidden{
	height: 40px;
	line-height: 40px;
	text-align: center;
	font-size: 14px;
}
/*按下效果*/
.hoverClass{
	background-color: #e7e5eb;
}


/*flex布局*/
.flex-flex{
  display: flex;
}
/*垂直居中*/
.flex-ver{
  display: flex;
  flex-flow: row nowrap;
  justify-content: flex-start;
  align-items: center;
}
/*垂直居中,水平两端对齐*/
.flex-between{
  display: flex;
  flex-flow: row nowrap;
  justify-content:space-between;
  align-items: center;
}
/*垂直上下两端对齐*/
.flex-column{
	display: flex;
  flex-direction: column;
  justify-content: space-between;
}
.flex-item{
  flex-grow: 1;
  text-align: center;
}
/* 页面产品--------展示公共 */
.list-cont{
	width: 732rpx;
	margin: 0 auto;
	overflow: hidden;
}
.list-pic {
    width: 342rpx;
    position: relative;
    overflow: hidden;
	border-radius: 25rpx;
	margin: 20rpx 12rpx 0;
}

.list-img {
    width: 342rpx;
	height: 249rpx;
	border-radius: 15rpx;
}

.list-tit-name {
    font-size: 24rpx;
    color: #121212;
    width: 342rpx;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}

.red-sign {
    position: absolute;
    left: 0;
    top: 210rpx;
    font-size: 16rpx;
    color: #fff;
    width: 95rpx;
    height: 28rpx;
	line-height: 28rpx;
	padding-left: 12rpx;
	background-image: url("data:image/png;base64,");
	background-size: 115rpx 28rpx;
}


.list-text {
	font-size: 16rpx;
	color: #7a7a7a;
	margin-bottom: 15rpx;
}
.list-text text{
	margin-right: 20rpx;
}
.list-text .list-yh {
	float: right;
	color: #ff0000;
	padding: 2rpx 5rpx 2rpx 10rpx;
	border: 1px solid #ff0000;
	border-radius: 25rpx;
}
.list-text .list-yh image{
	width: 15rpx;
	height: 15rpx;
	vertical-align: middle;
	margin-top: -2rpx;
