/*//搜索框*/

.search {
    height: 102.96rpx;
    width: 100%;
    padding: 0 32rpx;
    display: flex;
    flex-direction: row;
    align-items: center;
    box-sizing: border-box;
    position: fixed;
    z-index: 2;
    top: 130rpx;
    left: 0;
}

.search-box {
    /*width: 100%;*/
    height: 66rpx;
    font-size: 13px;
    line-height: 66rpx;
    box-sizing: border-box;
    border-radius: 35rpx;
    border: 1px solid #e6e6ea;
    background-color: #fff;
    display: flex;
    flex-direction: row;
    flex: 1;
}

.search-txt {
    flex: 1;
    height: 100%;
    line-height: 100%;
}

.search-pla {
    color: #b2b2b2;
    line-height: 100%;
}

.search-btn {
    width: 63.18rpx;
    height: 100%;
    display: flex;
    align-items: center;
    justify-content: center;
}

.search-btn1 {
    width: 63.18rpx;
    height: 100%;
    background: none;
    border: none;
    display: flex;
    align-items: center;
    justify-content: center;
    padding: 0;
}

.search-btn1::after {
    content: '';
    border: none;
}

.search-no {
    font-size: 16px;
    color: #1472e0;
    margin-left: 23.4rpx;
}

.fixed-box {
    height: 102.96rpx;
}

.fixed {
    position: fixed;
    top: 0;
    left: 0;
    z-index: 2;
    width: 100%;
    background-color: #efeff4;
}

.fixed-box {
    height: 205.92rpx;
}

.zhanwei {
    width: 100%;
    height: 106rpx;
}



