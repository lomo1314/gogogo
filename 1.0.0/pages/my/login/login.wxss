.logo{
	width: 230rpx;
	height: 300rpx;
	margin: 0 auto;
}
.logo image{
	width: 100%;
}
/*登录表单*/
.logo-box{
	width: 540rpx;
	margin: 0 auto;
}
.logo-box .section{
	width: 100%;
	height: 82rpx;
	border-bottom: 1px solid #f4f4f4;
	margin-bottom: 30rpx;
}
.logo-box .section input{
	flex: 1;
	height: 82rpx;
	padding-left: 30rpx;	
}
.section-icon image{
	width: 40rpx;	
	height: 40rpx;
}
.btn-area{
	padding: 0;
	margin-top: 20rpx;
}
.btnClick{
	background: #1472e0;
	color: #fff;
}
