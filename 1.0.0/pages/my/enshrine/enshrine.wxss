.editor,
.del {
    display: inline-block;
    width: 144rpx;
    height: 144rpx;
    text-align: center;
    line-height: 144rpx;
}

.editor {
    background: #ced0e2;
    color: #000;
}

.del {
    background: #eb2e2e;
    color: #fff;
}
.touch-move-active .listEns,
.touch-move-active .posit {
 -webkit-transform: translateX(0);
 transform: translateX(0);
}