.banner image {
    width: 100%;
    height: 326rpx;
    position: fixed;
    top: 0;
    left: 0;
    z-index: 1;
}

.page-til {
    font-size: 29rpx;
    color: #fefefe;
    position: fixed;
    top: 80rpx;
    left: 50%;
    transform: translate(-50%, -50%);
    z-index: 111;
}

.ify-listPage {
    width: 100%;
    position: fixed;
    left: 0;
    top: 150rpx;
    z-index: 121;
}

.ify-scroll {
    width: 100%;
}

.ify-liat {
    display: flex;
    flex-wrap: wrap;
}

.ifyboxs {
    display: inline-block;
    width: 150rpx;
    text-align: center;
}

.ify-name {
    font-size: 22rpx;
    color: #fff;
    margin-top: 22rpx;
    opacity: .5;
}
.ify-liat .active .ify-name {
    opacity: 1;
}
.swiper-box {
    display: block;
    height: 100%;
    width: 100%;
    overflow: hidden;
    padding-top: 361rpx;
    box-sizing: border-box;
}

.product {
    width: 732rpx;
    margin: 0 auto;
    overflow: hidden;
    background: #fff;
}

/* 首页样式 */

.hot-box {
    display: block;
    height: 100%;
}

.list-cont {
    width: 732rpx;
    margin: 0 auto;
    overflow: hidden;
    background: #fff;
}

.list-pic {
    width: 342rpx;
    position: relative;
    overflow: hidden;
    border-radius: 25rpx;
    margin: 20rpx 12rpx 0;
}

.list-img {
    width: 342rpx;
    height: 249rpx;
    border-radius: 15rpx;
}

.list-tit-name {
    font-size: 24rpx;
    color: #121212;
    width: 342rpx;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}

.red-sign {
    position: absolute;
    left: 0;
    top: 210rpx;
    font-size: 16rpx;
    color: #fff;
    width: 102rpx;
    height: 28rpx;
    line-height: 28rpx;
    padding-left: 12rpx;
    background-image: url("data:image/png;base64,");
    background-size: 115rpx 28rpx;
}

.list-text {
    font-size: 16rpx;
    color: #7a7a7a;
    margin-bottom: 15rpx;
}

.list-text text {
    margin-right: 20rpx;
}

.list-text .list-yh {
    float: right;
    color: #ff0000;
    padding: 2rpx 5rpx 2rpx 10rpx;
    border: 1px solid #ff0000;
    border-radius: 25rpx;
}

.list-text .list-yh image {
    width: 15rpx;
    height: 15rpx;
    vertical-align: middle;
    margin-top: -2rpx;
    margin-right: 3rpx;
}

.list-icon {
    width: 120rpx;
    height: 120rpx;
    background: #131313;
    border-radius: 50%;
    position: absolute;
    top: -60rpx;
    right: -60rpx;
}

.list-icon image {
    width: 17rpx;
    height: 17rpx;
    margin-top: 77rpx;
    margin-left: 28rpx;
}

/* 分类样式 */

.ify-icon {
    background-image: url(http://go.cnmo.com/static/images/iconIfy0823.png);
    background-size: 800rpx 160rpx;
    background-repeat: no-repeat;
    background-origin: center;
    margin: 0 auto;
    width: 88rpx;
    height: 69rpx;
    opacity: .5;
}
.active .ify-icon{
    opacity: 1;
}
