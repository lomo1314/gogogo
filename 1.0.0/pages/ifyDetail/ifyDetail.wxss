.banner image {
    width: 100%;
    height: 326rpx;
    position: fixed;
    top: 0;
    left: 0;
    z-index: 1;
}



.page-til {
    font-size: 29rpx;
    color: #fefefe;
    position: fixed;
    top: 80rpx;
    left: 50%;
    transform: translate(-50%, -50%);
    z-index: 111;
}

.ify-listPage {
    width: 100%;
    position: fixed;
    left: 0;
    top: 150rpx;
    z-index: 121;
}

.ify-scroll {
    width: 100%;
}

.ify-liat {
    display: flex;
    flex-wrap: wrap;
}

.ifyboxs {
    display: inline-block;
    width: 150rpx;
    text-align: center;
}

.ify-name {
    font-size: 22rpx;
    color: #fff;
    margin-top: 22rpx;
}

    

.swiper-box{ 
	display: block; 
	height: 100%; 
	width: 100%; 
	overflow: hidden;
	padding-top:361rpx;
	box-sizing: border-box;
}
